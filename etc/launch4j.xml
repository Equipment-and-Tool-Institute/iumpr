<?xml version="1.0" encoding="UTF-8"?>
<launch4jConfig>
  <headerType>gui</headerType>
  <jar>..\dist\IUMPR.jar</jar>
  <outfile>..\dist\IUMPR.exe</outfile>
  <chdir>.</chdir>
  <priority>high</priority>
	<jre>
		<path>jre</path>
		<initialHeapSize>256</initialHeapSize>
		<maxHeapSize>256</maxHeapSize>
		<opt>-server</opt>
		<opt>-XX:+UseG1GC</opt>
		<opt>-XX:MaxGCPauseMillis=4</opt>
	</jre>
</launch4jConfig>